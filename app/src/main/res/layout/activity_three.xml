<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ThreeMainActivity">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">
        <Button
            android:id="@+id/navigation_home"
            android:background="@drawable/button_select"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="100dp"
            app:layout_constraintRight_toLeftOf="@+id/navigation_dashboard"
            />
        <Button
            android:id="@+id/navigation_dashboard"
            android:background="@drawable/button_select"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginBottom="5dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            />
        <Button
            android:id="@+id/navigation_notifications"
            android:background="@drawable/button_select"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="100dp"
            app:layout_constraintLeft_toRightOf="@+id/navigation_dashboard"
            />

    </LinearLayout>


    <Button
        android:id="@+id/transmit_way1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:background="@drawable/acback"
        android:text="重启服务器"
        android:textColor="@color/textColorPrimary"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.331"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/transmit_way2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.501" />

    <Button
        android:id="@+id/transmit_way2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:background="@drawable/acback"
        android:text="发送自定义指令"
        android:textColor="@color/textColorPrimary"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.501" />

    <Button
        android:id="@+id/transmit_way3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:background="@drawable/acback"
        android:text="获取云端时间"
        android:textColor="@color/textColorPrimary"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.885"
        app:layout_constraintLeft_toLeftOf="@+id/transmit_way2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.501" />

    <Button
        android:id="@+id/transmit_way4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textColorPrimary"
        android:background="@drawable/acback"
        android:text="获取云端线程"
        android:textSize="15sp"
        android:layout_marginTop="350dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/transmit_way5"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <Button
        android:id="@+id/transmit_way5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textColorPrimary"
        android:background="@drawable/acback"
        android:text="获取云端IP"
        android:textSize="15sp"
        android:layout_marginTop="350dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <Button
        android:id="@+id/transmit_way6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textColorPrimary"
        android:background="@drawable/acback"
        android:text="关闭云端指定IP"
        android:textSize="15sp"
        android:layout_marginTop="350dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/transmit_way5"
        />
    <Button
        android:id="@+id/transmit_way7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textColorPrimary"
        android:background="@drawable/acback"
        android:text="关闭云端指定线程"
        android:textSize="15sp"
        android:layout_marginTop="550dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/transmit_way8"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <Button
        android:id="@+id/transmit_way8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textColorPrimary"
        android:background="@drawable/acback"
        android:text="清空黑名单"
        android:textSize="15sp"
        android:layout_marginTop="550dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <Button
        android:id="@+id/transmit_way9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textColorPrimary"
        android:background="@drawable/acback"
        android:text="关闭云端指定IP线程"
        android:textSize="15sp"
        android:layout_marginTop="550dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/transmit_way8"
        />
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="590dp"
        >
        <Button
            android:id="@+id/Sbu1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="卧室"
            android:textColor="@color/textColorPrimary"
            android:background="@drawable/acback"
            android:textSize="20dp"
            android:layout_marginTop="100dp"
            android:layout_marginLeft = "20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            />
        <SeekBar
            android:id="@+id/Sbu1_seekbar"
            android:layout_width="800dp"
            android:layout_height="40dp"
            android:max="100"
            android:progress="0"
            android:text="窗帘"
            android:textColor="@color/textColorPrimary"
            android:background="@drawable/acback"
            android:textSize="20dp"
            android:layout_marginTop="110dp"
            android:layout_marginLeft = "190dp"
            android:layout_marginRight = "20dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />
        <TextView
            android:id="@+id/Sbu1_seekbar_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="窗帘0%"
            android:textColor="@color/textColorPrimary"
            android:background="@drawable/acback"
            android:textSize="20dp"
            android:layout_marginTop="140dp"
            android:layout_marginLeft = "350dp"
            android:layout_marginRight = "20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            />
        <Button
            android:id="@+id/bedroom_Key"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="开关"
            android:textColor="@color/textColorPrimary"
            android:background="@drawable/acback"
            android:textSize="20dp"
            android:layout_marginTop="30dp"
            android:layout_marginLeft = "190dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            />
        <TextView
            android:id="@+id/bedroom_Key_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="灯关着呢"
            android:textColor="@color/textColorPrimary"
            android:background="@drawable/acback"
            android:textSize="20dp"
            android:layout_marginTop="40dp"
            android:layout_marginLeft = "440dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="590dp">
        <Button
            android:id="@+id/Sbu2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="厨房"
            android:textColor="@color/textColorPrimary"
            android:background="@drawable/acback"
            android:textSize="20dp"
            android:layout_marginTop="220dp"
            android:layout_marginLeft = "20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            />
        <Button
            android:id="@+id/kitchen_Key"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="开关"
            android:textColor="@color/textColorPrimary"
            android:background="@drawable/acback"
            android:textSize="20dp"
            android:layout_marginTop="220dp"
            android:layout_marginStart="80dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            />
        <TextView
            android:id="@+id/kitchen_Key_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="灯关着呢"
            android:textColor="@color/textColorPrimary"
            android:background="@drawable/acback"
            android:textSize="20dp"
            android:layout_marginTop="220dp"
            android:layout_marginStart="160dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="590dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <Button
            android:id="@+id/Sbu3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="340dp"
            android:background="@drawable/acback"
            android:text="厕所"
            android:textColor="@color/textColorPrimary"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />

        <Button
            android:id="@+id/toilet_Key"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="80dp"
            android:layout_marginTop="340dp"
            android:background="@drawable/acback"
            android:text="开关"
            android:textColor="@color/textColorPrimary"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/toilet_Key_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="160dp"
            android:layout_marginTop="340dp"
            android:background="@drawable/acback"
            android:text="灯关着呢"
            android:textColor="@color/textColorPrimary"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>